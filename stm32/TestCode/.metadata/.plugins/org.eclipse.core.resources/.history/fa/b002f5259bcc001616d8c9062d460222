#include "Drivetrain.h"
#include "Wheel.h"
#include <assert.h>

void Drivetrain_Initialize(void){
	Wheel_StopLeftWheel();
	Wheel_StopRightWheel();
}

void Drivetrain_FullStop(void) {
	Wheel_StopLeftWheel();
	Wheel_StopRightWheel();
}

int8_t Drivetrain_GetDirection(void) {

}

int8_t Drivetrain_GetVelocity(void);

}

void Drivetrain_Move(int8_t direction, int8_t velocity) {
	assert((direction >= -90) && (direction <= 90));
}
